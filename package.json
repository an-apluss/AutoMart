{
  "name": "automart",
  "version": "1.0.0",
  "description": "Auto Mart is an online marketplace for automobiles of diverse makes, model or body type. With Auto Mart, users can sell their cars or buy from trusted dealerships or private sellers.",
  "main": "index.js",
  "scripts": {
    "test": "mocha server/src/tests/*.js --require @babel/register --no-timeout --exit",
    "start": "nodemon --exec babel-node server/src/index.js",
    "babel-build": "babel server/src -d server/dist && node server/dist/index",
    "clean": "rm -rf server/dist",
    "build": "npm run clean && npm run babel-build",
    "heroku-postbuild": "echo skip post-build",
    "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
    "coveralls-coverage": "coveralls < lcov.info",
    "codeclimate-coverage": "codeclimate-test-reporter < lcov.info",
    "coverage": "nyc npm run test && npm run generate-lcov && npm run coveralls-coverage && npm run codeclimate-coverage",
    "create-tables": "babel-node server/src/database/migration/createTables",
    "drop-tables": "babel-node server/src/database/migration/dropTables",
    "user-seeder": "babel-node server/src/database/seeds/usersTableSeeder",
    "car-seeder": "babel-node server/src/database/seeds/carsTableSeeder",
    "order-seeder": "babel-node server/src/database/seeds/ordersTableSeeder",
    "flag-seeder": "babel-node server/src/database/seeds/flagsTableSeeder",
    "migration": "npm run drop-tables && npm run create-tables && npm run user-seeder && npm run car-seeder && npm run order-seeder && npm run flag-seeder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/an-apluss/AutoMart.git"
  },
  "keywords": [
    "automart",
    "online-marketplace",
    "selling",
    "buying",
    "automobile"
  ],
  "author": "an-apluss",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/an-apluss/AutoMart/issues"
  },
  "homepage": "https://github.com/an-apluss/AutoMart#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.0",
    "nyc": "^13.3.0",
    "prettier": "^1.17.1"
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@hapi/joi": "^15.0.3",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.14.0",
    "dotenv": "^7.0.0",
    "express": "^4.17.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "multer": "^1.4.1",
    "pg": "^7.11.0"
  }
}
